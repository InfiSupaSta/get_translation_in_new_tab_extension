{
  "manifest_version": 3,
  "author": "github.com/InfiSupaSta",
  "name": "Translate in new tab.",
  "version": "0.0.1",
  "description": "Open highlighted text in next tab for fast getting translate.",
  "web_accessible_resources": [
    {
      "resources": [
        "script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}